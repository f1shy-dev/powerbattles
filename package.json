{
    "name": "static",
    "version": "1.0.0",
    "description": "This is a static template with no bundling",
    "main": "index.html",
    "scripts": {
        "dev:twcss": "tailwindcss -i src/index.css -o src/dist.css -c tailwind.config.js --watch",
        "dev:serve": "serve src/",
        "dev": "run-p dev:*",
        "build": "echo This is a static template, there is no bundler or bundling involved!"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codesandbox-app/static-template.git"
    },
    "keywords": [
        "static",
        "template",
        "codesandbox"
    ],
    "author": "Ives van Hoorne",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/codesandbox-app/static-template/issues"
    },
    "homepage": "https://github.com/codesandbox-app/static-template#readme",
    "devDependencies": {
        "npm-run-all": "^4.1.5",
        "prettier-plugin-tailwind-css": "^1.5.0",
        "serve": "^11.2.0",
        "tailwindcss": "^3.0.7"
    }
}
